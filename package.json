    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "unplugin-icons": "^0.16.1",
    "vite": "^4.3.4",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-vue-markdown": "^0.23.4",
    "vite-ssg": "^0.22.2",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.6.4"
  },
  "volta": {
    "node": "18.16.0"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "vue/no-v-text-v-html-on-component": "off"
